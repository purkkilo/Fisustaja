    "concurrently": "^7.2.1",
    "nodemon": "^2.0.19"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "6.7",
    "mongoose": "^8.4.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0"
  }
}
